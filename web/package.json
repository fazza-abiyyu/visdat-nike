{
  "name": "nike-sales-dashboard",
  "version": "1.0.0",
  "description": "Complete Nike Sales Dashboard with Frontend and Backend",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:backend && npm run start:frontend",
    "start:backend": "cd backend && npm start",
    "start:frontend": "cd frontend && npm start",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "install:all": "npm run install:backend && npm run install:frontend",
    "install:backend": "cd backend && npm run install-deps",
    "install:frontend": "cd frontend && npm install",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && echo 'Backend build complete'",
    "test": "npm run test:backend && npm run test:frontend",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "deploy": "bash deploy.sh",
    "deploy:local": "bash deploy.sh local",
    "deploy:prod": "bash deploy.sh prod",
    "deploy:docker": "bash deploy.sh docker"
  },
  "keywords": [
    "nike",
    "sales",
    "dashboard",
    "visualization",
    "fastapi",
    "python",
    "javascript",
    "chart.js"
  ],
  "author": "Your Name",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/nike-sales-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/nike-sales-dashboard/issues"
  },
  "homepage": "https://github.com/yourusername/nike-sales-dashboard#readme"
}